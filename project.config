[project]
	description = OpenDaylight controller
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[access "refs/*"]
	owner = group ldap/odl-gerrit-controller-committers
	owner = group saml/odl-gerrit-controller-committers
	read = group GitHub Replication
[submit]
	mergeContent = true
	action = rebase if necessary
[allowSuperproject "releng/autorelease"]
	matching = refs/heads/stable/nitrogen:refs/heads/stable/nitrogen
