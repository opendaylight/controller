[project]
	state = active
	description = OpenDaylight controller
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group ldap/gerrit-controller-commiters
	label-Code-Review = -1..+1 group Non-Interactive Controller Users
	label-Code-Review = -1..+1 group Registered Users
	label-Verified = -1..+1 group Non-Interactive Controller Users
	label-Verified = -1..+1 group ldap/gerrit-controller-commiters
	submit = group ldap/gerrit-controller-commiters
	create = group Non-Interactive Controller Users
	push = group Non-Interactive Controller Users
	pushMerge = group Non-Interactive Controller Users
[access "refs/tags/*"]
	pushTag = group Non-Interactive Controller Users
	pushTag = group ldap/gerrit-controller-commiters
	create = group Non-Interactive Controller Users
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[access "refs/*"]
	owner = group ldap/gerrit-controller-commiters
	read = group Administrators
	read = group Anonymous Users
	read = group Non-Interactive Controller Users
	read = group Registered Users
	read = group ldap/gerrit-controller-commiters
	forgeAuthor = group ldap/gerrit-controller-commiters
[access "refs/for/refs/*"]
	push = group Registered Users
	push = group ldap/gerrit-controller-commiters
