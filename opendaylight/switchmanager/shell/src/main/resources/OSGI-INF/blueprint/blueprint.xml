<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <reference id="switchManagerRef" interface="org.opendaylight.controller.switchmanager.ISwitchManagerShell"/>
    <command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">

        <command>
        <action class="org.opendaylight.controller.switchmanager.shell.Pencs">
            <property name="switchManager" ref="switchManagerRef"/>
            </action>
        </command>

        <command>
        <action class="org.opendaylight.controller.switchmanager.shell.Pdm">
            <property name="switchManager" ref="switchManagerRef"/>
            </action>
        </command>

    </command-bundle>


</blueprint>
