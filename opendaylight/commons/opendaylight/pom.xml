      <id>ebr-bundles-external</id>
      <name>ebr-bundles-external</name>
      <url>${nexusproxy}/repositories/ebr-bundles-external/</url>
    </repository>
    <!-- Maven repo2 mirror -->
    <!-- http://repo2.maven.org/maven2 -->
    <repository>
      <id>central2</id>
      <name>central2</name>
      <url>${nexusproxy}/repositories/central2/</url>
    </repository>
